<!DOCTYPE html>
<html lang="{{ app.request.locale }}">
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}CNcCV{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('build/app.css') }}">
    {% endblock %}
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}
    <script src="{{ asset('build/manifest.js') }}"></script>
    <script src="{{ asset('build/vendor.js') }}"></script>
    <script src="{{ asset('build/app.js') }}"></script>

    <!--Google Map, ici pour Ã©viter les conflits-->
    <script>function initMap() {
            var leMansCentre = {lat: 48.004127, lng: 0.196683};
            var map = new google.maps.Map(document.getElementById('map'), {
                zoom: 15,
                center: leMansCentre
            });
            var marker = new google.maps.Marker({
                position: leMansCentre,
                map: map
            });
        }</script>
    <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBNiPj_WLCpWCzGKI02mV6ROO_PkwEn3yo&callback=initMap">
    </script>
{% endblock %}
</body>
</html>
